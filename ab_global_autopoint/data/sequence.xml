<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for job.order -->
        <record id="seq_work_order" model="ir.sequence">
            <field name="name">Joborder Sequence</field>
            <field name="code">job.order</field>
            <field name="prefix">JO/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>