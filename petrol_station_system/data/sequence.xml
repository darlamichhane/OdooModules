<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data noupdate="1">

		<!-- Sequences for transport.manifest -->
		<record id="seq_token_management" model="ir.sequence">
			<field name="name">Token Ref Sequence</field>
			<field name="code">token.ref.number.sequence</field>
			<field name="prefix">TOKEN/%(range_year)s/</field>
			<field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
			<field name="padding">4</field>
			<field name="company_id" eval="False" />
		</record>

		<!-- Sequences for sales.agreement -->
		<record id="seq_agreement_management" model="ir.sequence">
			<field name="name">Sale Agreement Sequence</field>
			<field name="code">sale.agreement.sequence</field>
			<field name="prefix">AGMT/%(range_year)s/</field>
			<field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
			<field name="padding">4</field>
			<field name="company_id" eval="False" />
		</record>

		<record id="seq_token_claim" model="ir.sequence">
			<field name="name">Token Sales Sequence</field>
			<field name="code">token.sales.sequence</field>
			<field name="prefix">CTSR/%(range_year)s/</field>
			<field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
			<field name="padding">4</field>
			<field name="company_id" eval="False" />
		</record>

		<record id="seq_token_number" model="ir.sequence">
					<field name="name">Token Number</field>
					<field name="code">token.number</field>
					<field name="prefix">TN/%(range_year)s/</field>
					<field eval="1" name="number_next"/>
            		<field eval="1" name="number_increment"/>
					<field eval="True" name="use_date_range"/>
					<field name="padding">5</field>
		</record>

	</data>
</odoo>